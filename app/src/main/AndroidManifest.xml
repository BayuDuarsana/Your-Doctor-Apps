<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ads.yourdoctorapps">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/doctor"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/doctor"
        android:supportsRtl="true"
        android:theme="@style/Theme.YourDoctorApps">
        <activity android:name=".YoutubeActivity"></activity>
        <activity android:name=".ToolsActivity" />
        <activity android:name=".Info2Activity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>